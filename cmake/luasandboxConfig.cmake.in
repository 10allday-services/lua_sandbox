set(LUASANDBOX_VERSION x.y.z)

@PACKAGE_INIT@

set_and_check(LUASANDBOX_INCLUDE_DIR "@PACKAGE_INCLUDE_INSTALL_DIR@")
set_and_check(LUASANDBOX_LIB_DIR "@PACKAGE_LIB_INSTALL_DIR@")

FIND_LIBRARY(LUASANDBOX_LUASB_LIBRARY   NAMES luasb             PATHS ${PACKAGE_LIB_INSTALL_DIR})
FIND_LIBRARY(LUASANDBOX_UTIL_LIBRARY    NAMES luasandboxutil    PATHS ${PACKAGE_LIB_INSTALL_DIR})
FIND_LIBRARY(LUASANDBOX_LIBRARY         NAMES luasandbox        PATHS ${PACKAGE_LIB_INSTALL_DIR})
FIND_LIBRARY(LUASANDBOX_HEKA_LIBRARY    NAMES luasandboxheka    PATHS ${PACKAGE_LIB_INSTALL_DIR})
FIND_PATH(LUASANDBOX_MODULES            NAMES modules           PATHS ${PACKAGE_LIB_INSTALL_DIR}luasandbox)

set(LUASANDBOX_LIBRARIES ${LUASANDBOX_HEKA_LIBRARY} ${LUASANDBOX_LIBRARY} ${LUASANDBOX_UTIL_LIBRARY} ${LUASANDBOX_LUASB_LIBRARY})
